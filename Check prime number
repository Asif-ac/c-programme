#include <stdio.h>

int main()
{
    int num,i;
    printf("Enter the number\n");
    scanf("%d",&num);

    int count=0;

    for (i=2;i<num;i++)
    {
        if(num%i==0)
        count++;
    }
    if(count!=0)
    {
        printf("Not a prime number\n");
    }
    else
    {
        printf("Prime number\n");
    }

    return 0;
}

