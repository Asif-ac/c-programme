#include<stdio.h>
int main()
{
    int i,j;
    int a[3][3],b[3][3],c[3][3];
    printf("First matrix\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d", &a[i][j]);
        }
    }


  printf("second matrix\n");
     for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d", &b[i][j]);
        }
    }


    printf("Print First matrix=\n");
     for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf(" %d ", a[i][j]);
        }
        printf("\n");
    }


    printf("Print second matrix = \n");
     for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf(" %d ", b[i][j]);
        }
         printf("\n");
    }


    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
           c[i][j]=a[i][j] + b[i][j];
        }

    }

 printf("The resultant of matrix = \n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
           printf(" %d ",c[i][j]);

        }
        printf("\n");

    }






    return 0;
}
